<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

    <navigation-rule>
        <from-view-id>/administracion/*</from-view-id>

        <navigation-case>
            <from-outcome>administracion/parametros/index.jsf</from-outcome>
            <to-view-id>/administracion/parametros/index.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>crearParametros</from-outcome>
            <to-view-id>/administracion/parametros/contenedor.jsf</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>administracion/configuracion/contenedor.jsf</from-outcome>
            <to-view-id>/administracion/configuracion/contenedor.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editarConfiguracion</from-outcome>
            <to-view-id>/administracion/configuracion/contenedor.jsf</to-view-id>
        </navigation-case>
        
    </navigation-rule>

</faces-config>
