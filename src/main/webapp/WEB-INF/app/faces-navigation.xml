<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

    <navigation-rule>
        <from-view-id>/app/*</from-view-id>

        <navigation-case>
            <from-outcome>app/bundle/index.jsf</from-outcome>
            <to-view-id>/app/bundle/index.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>crearBundle</from-outcome>
            <to-view-id>/app/bundle/contenedor.jsf</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>app/propiedad/index.jsf</from-outcome>
            <to-view-id>/app/propiedad/index.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>crearPropiedad</from-outcome>
            <to-view-id>/app/propiedad/contenedor.jsf</to-view-id>
        </navigation-case>

     	<navigation-case>
            <from-outcome>app/persona/index.jsf</from-outcome>
            <to-view-id>/app/persona/index.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>crearPersona</from-outcome>
            <to-view-id>/app/persona/contenedor.jsf</to-view-id>
        </navigation-case>
     </navigation-rule>   

</faces-config>
