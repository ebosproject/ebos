<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

    <navigation-rule>
        <from-view-id>/seguridad/*</from-view-id>

        <navigation-case>
            <from-outcome>seguridad/usuario/index.jsf</from-outcome>
            <to-view-id>/seguridad/usuario/index.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>crearUsuario</from-outcome>
            <to-view-id>/seguridad/usuario/contenedor.jsf</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>seguridad/rol/index.jsf</from-outcome>
            <to-view-id>/seguridad/rol/index.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>crearRol</from-outcome>
            <to-view-id>/seguridad/rol/contenedor.jsf</to-view-id>
        </navigation-case>
                
        <navigation-case>
            <from-outcome>seguridad/opcion/index.jsf</from-outcome>
            <to-view-id>/seguridad/opcion/index.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>crearOpcion</from-outcome>
            <to-view-id>/seguridad/opcion/contenedor.jsf</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>seguridad/objeto/index.jsf</from-outcome>
            <to-view-id>/seguridad/objeto/index.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>crearObjeto</from-outcome>
            <to-view-id>/seguridad/objeto/contenedor.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>
